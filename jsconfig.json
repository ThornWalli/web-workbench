{
  "extends": "./packages/app/.nuxt/tsconfig.json",
  "compilerOptions": {
    "target": "esnext",
    "module": "es2015",
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./app/src/*"
      ],
      "@@/*": [
        "./app/*/"
      ],
      "@web-workbench/app/*": [
        "./app/*"
      ],
      "@web-workbench/core/*": [
        "./core/*"
      ],
      "@web-workbench/disks/*": [
        "./disks/*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}